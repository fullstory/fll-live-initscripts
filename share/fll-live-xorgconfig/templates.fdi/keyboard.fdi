<?xml version="1.0" encoding="UTF-8"?>
<deviceinfo version="0.2">
	<device>
			<match key="info.capabilities" contains="input.keys">
			<merge key="input.x11_driver" type="string">evdev</merge>
			<merge key="input.x11_options.XkbLayout" type="string">@@XKBLAYOUT@@</merge>
			<merge key="input.x11_options.XkbModel" type="string">@@XKBMODEL@@</merge>
			<merge key="input.x11_options.XkbOptions" type="string">@@XKBOPTIONS@@</merge>
		</match>
	</device>
</deviceinfo>
